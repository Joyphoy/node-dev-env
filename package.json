{
  "name": "node-dev-env",
  "version": "1.0.0",
  "description": "Node.js development environment",
  "author": "Joonas Siltala",
  "license": "MIT",
  "repository": "Joyphoy/node-dev-env",
  "keywords": [
    "node",
    "development",
    "environment"
  ],
  "main": "scripts/server",
  "scripts": {
    "prestart": "node scripts/message",
    "start": "npm-run-all --parallel security-check dev-server",
    "dev-server": "node scripts/server",
    "test": "echo \"Error: no test specified\" && exit 1",
    "security-check": "nsp check",
    "localtunnel": "lt --port 3000 --subdomain joyphoy",
    "share": "npm-run-all --parallel dev-server localtunnel"
  },
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-preset-latest": "^6.16.0",
    "chalk": "^1.1.3",
    "express": "^4.14.0",
    "localtunnel": "^1.8.2",
    "npm-run-all": "^3.1.2",
    "nsp": "^2.6.2",
    "open": "0.0.5"
  }
}
